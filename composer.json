{
  "name": "maatwebsite/excel",
  "description": "An eloquent way of importing and exporting Excel and CSV",
  "license": "LGPL",
  "keywords": [
    "laravel",
    "php",
    "spout",
    "phpspreadsheet",
    "phpexcel",
    "excel",
    "csv",
    "export",
    "import",
    "batch"
  ],
  "authors": [
    {
      "name": "Maatwebsite.nl",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": ">=7.0",
    "phpoffice/phpspreadsheet": "dev-develop"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.2",
    "illuminate/support": "5.*",
    "illuminate/filesystem": "5.*",
    "codeclimate/php-test-reporter": "^0.4.4"
  },
  "autoload": {
    "psr-4": {
      "Maatwebsite\\Excel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maatwebsite\\Excel\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "@php vendor/bin/phpunit"
    ],
    "coverage": [
      "@php -dzend_extension=xdebug.so vendor/bin/phpunit --coverage-html=tests/_data/tmp/coverage"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Maatwebsite\\Excel\\Bridge\\Laravel\\ExcelServiceProvider"
      ],
      "aliases": {
        "Excel": "Maatwebsite\\Excel\\Bridge\\Laravel\\Facades\\Excel"
      }
    }
  }
}
